# Useful aliases to be loaded by zshrc.
# Assumes oh-my-zsh is already installed.
#
# Usage:
#   source aliases
###################################################

# VScode
alias c.='code .'

# zsh
alias zshconfig="code ~/.zshrc"

# Node
alias nb='npm run build'
alias ni='npm install'
alias ns='npm start'
alias nsd='npm start:dev'
alias nt='npm test'
alias nr='npm run'

# Ruby
alias bi='bundle install'

# Web
alias gmail='open_command https://mail.google.com/mail/u/1/'
